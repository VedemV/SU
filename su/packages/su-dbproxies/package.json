{
    "name": "su-dbproxies",
    "namespace": "SU.dbproxies",
    "type": "code",
    "creator": "skillunion",
    "summary": "ExtJS proxies for Websql/Sqlite and IndexedDB",
    "detailedDescription": "ExtJS proxies for Websql/Sqlite and IndexedDB",
    "version": "1.0.1.207",
    "compatVersion": "1.0.1",
    "format": "1",
    "framework": "ext",
    "builds": {
        "su-dbproxies-classic": {
            "toolkit": "classic",
            "name": "su-dbproxies-classic"
        },
        "su-dbproxies-modern": {
            "toolkit": "modern",
            "name": "su-dbproxies-modern"
        }
    },

    "slicer": {
        "js": [
            {
                "path": "${package.dir}/sass/example/custom.js",
                "isWidgetManifest": true
            }
        ]
    },

    "output": "${workspace.packages.dir}/build",
    "local": true,
    "sass": {

    },

    "classpath": [
        "${package.dir}/src"
    ],

    "overrides": [
        "${package.dir}/src/overrides"
    ],

    "example": {
        "path": [
            "${package.dir}/examples"
        ]

    },

    "requires": [],

    "properties": {
        "skip.js": 0,
        "skip.sass": 1,
        "skip.slice": 1,
        "skip.examples": 1,
        "pkg.build.dir": "${workspace.build.dir}/pkgs",
        "pkg.file.name": "su-dbproxies.pkg"
    }

}
