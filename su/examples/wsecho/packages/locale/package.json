{
    "framework": "ext",
    "name": "locale",
    "namespace": "SU",
    "format": "1",
    "toolkit": "classic",
    "version": "1.0.1.201",
    "compatVersion": "1.0.1",
    "creator": "skillunion",
    "summary": "SU locale",
    "detailedDescription": "Skill Union locale package",
    "type": "code",
    "local": true,
    "output": "${package.dir}/../../resources/locale",
    "classpath": [
        "${package.dir}/src"
    ],
    "overrides": [
        "${package.dir}/overrides/${package.locale}"
    ],
    "compressor": {
        "polyfills": "none"
    },

    "requires": [],
    "subpkgs": null,
    
    "properties": {
        "skip.sass": 1,
        "skip.slice": 1,
        "skip.examples": 1,
        "skip.pkgs": 1
        //"pkg.build.dir": "${workspace.build.dir}/pkgs",
        //"pkg.file.name": "su-locale.pkg"
    }
}
